  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "test": "cross-env VUE_CLI_MODE=test mochapack --recursive --r tests/setup.js --webpack-config ./node_modules/@vue/cli-service/webpack.config.js  tests/unit/**/*.spec.js",
    "lint": "vue-cli-service lint",
    "build-dev": "concurrently -p \"ðŸš€\" \"npm run build-matataki-dev\" \"npm run build-widget-dev\" \"npm run build-token-dev\" \"npm run build-user-dev\"",
    "build-matataki-dev": "vue-cli-service build --mode staging",
    "build-matataki-prod": "vue-cli-service build --mode production",
    "build-prod": "concurrently -p \"ðŸš€\" \"npm run build-matataki-prod\" \"npm run build-widget-prod\" \"npm run build-token-prod\" \"npm run build-user-prod\"",
    "build-token-dev": "cd token && npm i && npm run build-dev && cd ..",
    "build-token-prod": "cd token && npm i && npm run build-prod && cd ..",
    "build-user-dev": "cd user && npm i && npm run build-dev && cd ..",
    "build-user-prod": "cd user && npm i && npm run build-prod && cd ..",
    "build-widget-dev": "cd widget && npm i && npm run build-dev && cd ..",
    "build-widget-prod": "cd widget && npm i && npm run build-prod && cd ..",
    "svgo": "svgo -f src/icons/filter --config=src/icons/svgo.yml"
