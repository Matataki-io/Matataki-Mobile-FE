{
  "name": "smart-signature-future",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development ",
    "build-dev": "vue-cli-service build --mode development",
    "build-prod": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "build-for-netlify": "npm run build-dev && npm run fix-netlify-cs-routing",
    "build-for-surge": "npm run build && npm run fix-surge-cs-routing",
    "fix-netlify-cs-routing": "echo '/*    /index.html   200' >> ./dist/_redirects",
    "fix-surge-cs-routing": "mv ./dist/index.html ./dist/200.html"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "clipboard": "^2.0.4",
    "eosjs": "16.0.9",
    "iview": "^3.3.0",
    "mavon-editor": "^2.7.0",
    "moment": "^2.24.0",
    "qs": "^6.6.0",
    "register-service-worker": "^1.5.2",
    "request": "^2.88.0",
    "scatterjs-core": "^2.7.16",
    "scatterjs-plugin-eosjs": "^1.5.1",
    "vue": "^2.6.6",
    "vue-infinite-scroll": "^2.0.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "wangeditor": "^3.1.1",
    "zarm-vue": "2.0.0-alpha.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-pwa": "^3.0.1",
    "@vue/cli-service": "^3.5.3",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-cli-plugin-iview": "^1.0.6",
    "vue-template-compiler": "^2.5.21"
  }
}
